<%- include('header'); -%>

<body class="bg-gradient-primary""">
    <div class="containerDetails">

        <div class="card o-hidden border-0 shadow-lg my-5">
            <div class="card-body p-0">
                <div class="row">
                    <div class="col-lg-5 d-none d-lg-block bg-voting-image"></div>
                    <div class="col-lg-7">
                        <div class="p-5 ">
                            <ul>
                                <% for(var i=0; i<ballotDetails.length; i++) {%>
                                <div class="text-center">
                                    <h1 id="detailsH1" class="h4 text-gray-900 mb-4">
                                        <%- ballotDetails[i].official_title %></h1>
                                </div>

                                <li class="detailListStyle">
                                    <p id="detailSummary"><%- ballotDetails[i].summary %></p>
                                    <div class="flexin">

                                        <div class="page-header text-center blue">
                                        </div>



                                        <div class="profileCardRightDetail">
                                            <div class="jumbotronDetails text-center">
                                                <div id="center" class="well offWhite">
                                                    <img id="profilePicture" class="img-fluid rounded-circle mb-3"
                                                        src="data:image/svg+xml;base64,"
                                                        alt="">
                                                </div>
                                                <div class="text-center">
                                                    <h1 class="h4 text-gray-900 mb-4">Vote Yes</h1>
                                                </div>

                                                <p><%- ballotDetails[i].yes_vote %></p>
                                            </div>
                                            <hr>
                                        </div>



                                        <div class="profileCardLeftDetail">
                                            <div class="jumbotronDetails text-center">
                                                <div>
                                                    <div id="center" class="well offWhite">
                                                        <img id="profilePicture" class="img-fluid rounded-circle mb-3"
                                                            src="data:image/svg+xml;base64,"
                                                            alt="">
                                                    </div>
                                                    <div class="text-center">
                                                        <h1 class="h4 text-gray-900 mb-4">Vote No</h1>
                                                    </div>
                                                    <p><%- ballotDetails[i].no_vote %></p>
                                                </div>
                                                <hr>
                                            </div>
                                        </div>
                                    </div <div>
                                    <p> Status: <%- ballotDetails[i].status %></p>
                                    <p>Location: <%- ballotDetails[i].district_name %></p>
                                    <p>District Type: <%- ballotDetails[i].district_type %></p>
                                    <p>District ID: <%- ballotDetails[i].district_id %></p>
                        </div>

                        </li>
                        <% } %>
                        </ul>

                        <hr>
                    </div>
                </div>
            </div>
        </div>




    </div>
    </div>

    <!-- Bootstrap core JavaScript-->
    <script src="vendor/jquery/jquery.min.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="vendor/jquery-easing/jquery.easing.min.js"></script>

    <!-- Custom scripts for all pages-->
    <script src="js/sb-admin-2.min.js"></script>
    <script src="/followBallot.js"></script>
</body>
<%- include('footer'); -%>